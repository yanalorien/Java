package homeworks;

public class H5
{
	public void fibonacciLoop(int n)
	{
		if (n >= 2)
		{
			System.out.print("0 1 ");
			int f0 = 0;
			int f1 = 1;
			int fn = 0;
			
			for (int i = 2; i < n; i++)
			{
				fn = f1 + f0;
				System.out.print(fn + " ");
				f0 = f1;
				f1 = fn;
			}
		}
		else 
			System.out.print(n - 1);
	}
	public static void main(String[] args)
	{
		H5 h = new H5();
		h.fibonacciLoop(2);
	}
}
