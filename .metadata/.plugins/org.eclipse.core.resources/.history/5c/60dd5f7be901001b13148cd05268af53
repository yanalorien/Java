package controls;
public class IfElse
{
	public static void main(String[] args)
	{
		boolean condition = true;
		
		if (condition)
			System.out.println("Inside if");
		else
			System.out.println("Inside else");
	}
}
// when age is 21 and over, you can drink alcohol
// test when age = 18 and age = 21 and age = 68;