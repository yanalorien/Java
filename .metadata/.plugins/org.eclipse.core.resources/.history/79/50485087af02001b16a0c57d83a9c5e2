package homeworks;

public class Monkeys
{
	public static void main(String[] args)
	{
		boolean aSmile = true;
		boolean bSmile = false;
		
		if (aSmile == true && bSmile == true || aSmile == false && bSmile == false)
			System.out.println("We have trouble");
		else
			System.out.println("Everything is good");
	}	
}
